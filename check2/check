PROCEDURE scalaire(VIT,V2:ARRAY_OF INTEGER)
VAR
    mun : INTEGER ;
BEGIN
    mun :=
END
ALGORITHM main
VAR
    scalaire : PROCEDURE;
    V1, V2 : ARRAY_OF INTEGER [3];
    i : INTEGER ; ;
BEGIN
    FOR i FROM 0 TO 2 STEP step DO
        Read(V1[i])
        Read(v2[i])
    END_FOR
    IF (scalaire(V1,V2)==0) THEN
        write ("V1 et v2 sont orthogonal")
    ELSE
        write ("they are not ")
    END_IF
END
